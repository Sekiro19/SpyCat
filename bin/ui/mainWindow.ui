/*_______________Fixed_Styles______________*/
/*QLineEdit*/
QLineEdit {
	border: 2px solid rgb(103, 103, 103);
	border-radius: 5px;
	background-color: Transparent;
	color: rgb(121, 121, 121);
	padding-left: 10px;
	padding-right: 10px;
	background-repeat: none;
	background-position: left center;
}
QLineEdit:focus {
	color: rgb(230, 230, 230);
	border: 2px solid rgb(189, 255, 0);
}
/*QPushButton*/

/*QScrollBar*/
QScrollBar:horizontal {
    border: none;
    background: rgb(52, 59, 72);
    height: 8px;
    margin: 0px 21px 0 21px;
	border-radius: 4px;
}
QScrollBar::handle:horizontal {
    background: rgb(189, 255, 0);
    min-width: 25px;
	border-radius: 4px
}
QScrollBar::add-line:horizontal {
	border: none;
    background: transparent;
	width: 20px;
	border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
	border: none;
    background: transparent;
    width: 20px;
	border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
     background: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
     background: none;
}
 QScrollBar:vertical {
	border: none;
    background: rgb(52, 59, 72);
    width: 8px;
    margin: 21px 0 21px 0;
	border-radius: 4px;
 }
 QScrollBar::handle:vertical {	
	background: rgb(189, 255, 0);
    min-height: 25px;
	border-radius: 4px
 }
 QScrollBar::add-line:vertical {
     border: none;
    background: transparent;
     height: 10px;
    border-radius: 4px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:vertical {
	border: none;
    background: transparent;
    height: 10px;
    border-radius: 4px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     background: none;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }
/*QTabWidget*/
QTabWidget::pane { 
	top:-1px;
	border: none;
	background-color: transparent;
}
QTabWidget::tab-bar {
	border: none;
	top:-1px; 
}
QTabBar::tab {
	background-color: transparent;
	font: 700 11pt &quot;Roboto&quot;;
	color: rgb(255, 255, 255);
	width: 150px; height: 30px;
 }
QTabBar::tab:selected {
	border-top: 2px solid  rgb(189, 255, 0);
 }
/*QComboBox*/
QComboBox{
	background-color: Transparent;
	color: rgb(230, 230, 230);
	background-repeat: none;
	background-position: left center;
	border: 2px solid rgb(103, 103, 103);
	border-radius: 5px;
	padding: 5px;
	padding-left: 10px;
}
QComboBox:hover{
	color: rgb(230, 230, 230);
	border: 2px solid rgb(189, 255, 0);
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px; 
	border-left-width: 3px;
	border-left-color: rgba(39, 44, 54, 150);
	border-left-style: solid;
	border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;	
	background-image: url(:/all/bin/images/cil-arrow-bottom.png);
	background-position: center;
	background-repeat: no-reperat;
 }
QComboBox QAbstractItemView {
	color: rgb(230, 230, 230);
	background-color: rgb(33, 37, 43);
	padding: 10px;
	selection-background-color: rgb(39, 44, 54);
}
QProgressBar {
    background: rgb(112, 118, 130);
	color: rgb(233, 12, 89);
	font: 7pt &quot;Roboto&quot;;
	text-align: right;
    border-radius: 4px;
	margin-right: 20px;
}
QProgressBar::chunk {
    background-color: rgb(189, 255, 0);
    border-radius: 3px;
}

QLabel {
	border-radius: 5px;
	font: 700 11pt &quot;Roboto&quot;;
	color: rgb(255, 255, 255);
}
/*QPlainTextEdit*/
